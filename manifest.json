{
  "manifest_version": 2,
  "name": "Mercado ivre Frete",
  "description": "Exibe o valor de frete de um item do MMercado Livre ao lado do valor do produto.",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/emoji-substitution",
  "version": "0.0.1",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
